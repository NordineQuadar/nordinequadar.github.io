[31m-@function fa-divide($dividend, $divisor, $precision: 10) {[m
[31m-  $sign: if($dividend > 0 and $divisor > 0, 1, -1);[m
[31m-  $dividend: abs($dividend);[m
[31m-  $divisor: abs($divisor);[m
[31m-  $quotient: 0;[m
[31m-  $remainder: $dividend;[m
[31m-  @if $dividend == 0 {[m
[31m-    @return 0;[m
[31m-  }[m
[31m-  @if $divisor == 0 {[m
[31m-    @error "Cannot divide by 0";[m
[31m-  }[m
[31m-  @if $divisor == 1 {[m
[31m-    @return $dividend;[m
[31m-  }[m
[31m-  @while $remainder >= $divisor {[m
[31m-    $quotient: $quotient + 1;[m
[31m-    $remainder: $remainder - $divisor;[m
[31m-  }[m
[31m-  @if $remainder > 0 and $precision > 0 {[m
[31m-    $remainder: fa-divide($remainder * 10, $divisor, $precision - 1) * 0.1;[m
[31m-  }[m
[31m-  @return ($quotient + $remainder) * $sign;[m
[31m-}[m
